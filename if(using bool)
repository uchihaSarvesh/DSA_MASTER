#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    bool ismale = true;
    bool istall = true;
    if(ismale && istall)
    {
        cout<<"You are a tall male";
    }
    else if(!ismale && istall)
    {
        cout<<"Either you are not a male and you are tall";
    }
    else if(ismale && !istall)
    {
        cout<<"You are male but not tall";
    }
    else(!ismale && !istall)
    {
        cout<<"You are not a male are aren't tall"
    }
    
    
   return 0;
}
